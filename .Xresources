#include ".cache/squash/x_colors"
#include "/home/winston/.xrdb/colors/monodevek"
#include "/home/winston/.xrdb/fonts/tewi"
!#include "/home/winston/.xrdb/rofi/monodevek"

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
XTerm.vt100.faceName: Bittypix Monospace:size=11:antialias=true
Xft.rgba: rgb
Xft.dpi: 96
XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.vt100.selectToClipboard: true
Xcursor*theme: Adwaita

URxvt.scrollBar: false
!URxvt*depth: 32
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt*borderless: false
!URxvt.letterSpace: 1
